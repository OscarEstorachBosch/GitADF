{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdl"
		},
		"LSAzureBlobStorageConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSAzureBlobStorageConnection'"
		},
		"LSAzureDataLakeStorageConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LSAzureDataLakeStorageConnection'"
		},
		"LSAzureSqlDatabase_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSAzureSqlDatabase_connection'"
		},
		"LS_AzureBlobStorageDemo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureBlobStorageDemo'"
		},
		"LS_BlobStorage_PJA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobStorage_PJA'"
		},
		"LSAzureDataLakeStorageConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageadfdl.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LSAzureBlobStorageConnection_connectionString": {
						"value": "[parameters('LSAzureBlobStorageConnection_connectionString')]"
					},
					"LSAzureDataLakeStorageConnection_accountKey": {
						"value": "[parameters('LSAzureDataLakeStorageConnection_accountKey')]"
					},
					"LSAzureSqlDatabase_connection_connectionString": {
						"value": "[parameters('LSAzureSqlDatabase_connection_connectionString')]"
					},
					"LS_AzureBlobStorageDemo_connectionString": {
						"value": "[parameters('LS_AzureBlobStorageDemo_connectionString')]"
					},
					"LS_BlobStorage_PJA_connectionString": {
						"value": "[parameters('LS_BlobStorage_PJA_connectionString')]"
					},
					"LSAzureDataLakeStorageConnection_properties_typeProperties_url": {
						"value": "[parameters('LSAzureDataLakeStorageConnection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}